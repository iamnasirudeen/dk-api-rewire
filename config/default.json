{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "testPort": 4040,
  "supportedDevices": [
    "mobile-android",
    "mobile-ios",
    "mobile-browser",
    "desktop",
    "generic"
  ],
  "unsetErrorKey": "unsetErrorToResult",
  "supportedRoles": [
    {
      "name": "Super Admin"
    },
    {
      "name": "Admin"
    },
    {
      "name": "Users Moderator"
    },
    {
      "name": "Posts Moderator"
    },
    {
      "name": "Media Moderator"
    },
    {
      "name": "Basic User"
    }
  ],
  "supportedAccountTypes": [
    {
      "name": "Admin",
      "published": true
    },
    {
      "name": "Moderator",
      "published": true
    },
    {
      "name": "Individual",
      "published": true
    },
    {
      "name": "Couple/Group",
      "published": true
    }
  ],
  "unProtectedPaths": [
    "api-keys"
  ],
  "trustedSources": [
    "postman",
    "test"
  ],
  "idempotentKey": "actionId",
  "tests": {
    "environmentsAllowingSeedData": [
      "test"
    ]
  },
  "mongodb": "mongodb://localhost:27017/dating_kinky_api",
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  }
}
